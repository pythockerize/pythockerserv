      "iam:CreateRole",

      "iot:DisableTopicRule",
      "iot:CreateTopicRule",
      "iot:ReplaceTopicRule",
      "iot:DeleteTopicRule",
      "iot:EnableTopicRule",

      "kinesis:DescribeStream",
      "kinesis:CreateStream",
      "kinesis:DeleteStream",

